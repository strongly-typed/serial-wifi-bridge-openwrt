config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'

config wifi-iface 'wlan0'
	option device 'radio0'
	option network 'wifi'
	option mode 'sta'
	option ssid 'SSID'
	option encryption 'psk2'
	option key 'PASSWORD'
